/*! reset */
body {
  background: none repeat scroll 0 0 #EFEFEF;
  overflow: hidden;
  position: relative;
  margin: 0px;
  padding: 0px;
}
html, body {
  height: 100%;
}
div, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, pre, form, fieldset, input, textarea, p, blockquote, th, td {
  margin: 0;
  padding: 0;
}
fieldset {
  border: 0 none;
}
